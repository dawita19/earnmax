{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "dev:debug": "nest start --watch --debug --preserveWatchOutput",
    "start": "nest start",
    "start:prod": "node dist/main",
    "start:cluster": "pm2 start dist/main.js -i max",
    "build": "nest build",
    "build:prod": "npm run build && npm prune --production",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --silent",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
    "test:stress": "artillery run ./test/load-test.yml",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix --max-warnings=0",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "typecheck": "tsc --noEmit --incremental",
    "migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migrate:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "migrate:gen": "npm run typeorm -- migration:generate -n",
    "seed": "ts-node ./database/seeders/main.ts",
    "docker:build": "docker build --no-cache -t backend .",
    "docker:run": "docker run -p 3000:3000 -p 9229:9229 backend",
    "docker:compose": "docker-compose up --build",
    "healthcheck": "curl -f http://localhost:3000/health || exit 1",
    "docs": "npm run build && swagger-ui-express ./swagger.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.0",
    "@nestjs/swagger": "^6.0.0",
    "@nestjs/terminus": "^9.0.0",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "@opentelemetry/api": "^1.0.0",
    "@opentelemetry/instrumentation": "^0.40.0",
    "@sentry/node": "^7.0.0",
    "bcrypt": "^5.0.0",
    "cache-manager": "^5.0.0",
    "cache-manager-redis-store": "^3.0.0",
    "class-validator": "^0.14.0",
    "compression": "^1.7.0",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-rate-limit": "^6.0.0",
    "helmet": "^6.0.0",
    "hpp": "^0.2.0",
    "ioredis": "^5.0.0",
    "jsonwebtoken": "^9.0.0",
    "kafkajs": "^2.0.0",
    "mongoose": "^7.0.0",
    "mysql2": "^3.0.0",
    "nest-jsx-email": "^0.5.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.0.0",
    "pm2": "^5.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.0.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.0",
    "typeorm-extension": "^2.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.0",
    "@types/express-rate-limit": "^5.0.0",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.0.0",
    "@types/passport-jwt": "^3.0.0",
    "@types/supertest": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "artillery": "^2.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^6.0.0",
    "jest": "^29.0.0",
    "jest-extended": "^3.0.0",
    "prettier": "^2.0.0",
    "supertest": "^6.0.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.0.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "setupFilesAfterEnv": ["jest-extended/all"],
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.dto.ts",
      "!**/*.entity.ts",
      "!**/*.module.ts",
      "!**/main.ts"
    ],
    "coverageDirectory": "../coverage",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testEnvironment": "node"
  }
}